<html>
  <head>
    <!-- Set the Title in the Browser Window Bar -->
    <title>Square Cafe</title>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/themes/black-tie/jquery-ui.min.css" />
    <link rel="stylesheet" type="text/css" href="square.css" />
    <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
    <script src="custom.js"></script>
<!--     <script type="square.js"></script> -->
  </head>
  <body>
    <div id="dialog-modal" title="Sign Up">
      <h4>Sign Up For Our Shit!</h4>
      <form id="signup">
          <input type="text" name="email" id="email" placeholder="Enter Your Email Address"/>
      </form>
    </div>

    <div id="headmast">
      <h1>Square Cafe</h1>
      <h2>716 Congress, Suite 404, Austin, TX</p>
    </div>

    <div id="main">
      <div id="sidebar">
        <div id="picture">
          <img src="cappucino.jpg" width="160" />
        </div>

        <a class="twitter-timeline" href="https://twitter.com/MakerSquareCafe" data-widget-id="353241485782700032">Tweets by @MakerSquareCafe</a>
        <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+"://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>

      </div>
      <div class="panel">
        <h2>What Would You Like?</h2>


        <form id ="drinkform">
          <div id="drinkdiv">
            <!-- See this "drink[]" name? -->
            <!-- This is turned into an array of items -->
            <select name="drink[]">
              <option>Please select a drink</option>
              <optgroup label="Coffee">
                <option data-price="200" data-image="housecoffee.jpeg" value="House Coffee">House Coffee — $2.00</option>
                <option data-price="200" data-image="icedcoffee.jpeg" value="Iced Coffee">Iced Coffee — $2.00</option>
                <option data-price="200" data-image="boldcoffee.jpeg" value="Bold Coffee">Bold Coffee — $2.00</option>
              </optgroup>
              <optgroup label="Espresso">
                <!-- See this 'data-price' attribute? -->
                <!-- We can create arbitrary attributes in HTML5 by prepending the attribute name with 'data-' -->
                <option data-price="300" data-image="americano.jpeg" value="Americano">Americano — $3.00</option>
                <option data-price="200" data-image="espresso.jpeg" value="Espresso">Espresso — $2.00</option>
                <option data-price="350" data-image="cappucino2.jpeg" value="Cappucino">Cappucino — $3.50</option>
              </optgroup>
              <optgroup label="Teas">
                <option data-price="300" data-image="greentea.jpeg" value="Green Tea">Green Tea — $3.00</option>
                <option data-price="300" data-image="blacktea.jpeg" value="Black Tea">Black Tea — $3.00</option>
                <option data-price="300" data-image="peachtea.jpeg" value="Peach Tea">Peach Tea — $3.00</option>
              </optgroup>
            </select>
          </div>
          <dl>
            <dt># of Drinks</dt>
            <dd id="drinks">0</dd>

            <dt>Taxes</dt>
            <dd id="taxes">$0.00</dd>

            <dt>Cost</dt>
            <dd id="cost">$0.00</dd>
          </dl>
          <br>
          <input id="submitbutton" type="submit" value="Submit Your Order">
        </form>
      </div>

